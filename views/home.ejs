        </section>
    
        <section id="work-section">
            <h2>Featured Work</h2>
            <div id="wcard-container">

                <div class="wcard" onclick="clickHREF(this)">
                    <img src="/assets/Screenshot from 2025-02-08 17-54-57.png" alt="Domifactus agency">
                    <a href="https://markos-souchlos.github.io/architecture/en/home.html" target="_blank">Domifactus Agency</a>
                </div>

                <div class="wcard" onclick="clickHREF(this)">
                    <img src="assets/gym-aiantas.png" alt="gym project">
                    <a href="https://markos-souchlos.github.io/gym-website/" target="_blank">Gym website Aiantas</a>
                </div>
            
                
                <div class="wcard" onclick="clickHREF(this)">
                    <img src="assets/nutrition-tracker.jpg" alt="macros tracker project">
                    <a href="/work/macros-tracker" target="_blank">Macros tracker</a>
                </div>
                
                <div class="wcard" id="third-wc" onclick="clickHREF(this)">
                    <img src="assets/meal-planner.png" alt="meal planner project">
